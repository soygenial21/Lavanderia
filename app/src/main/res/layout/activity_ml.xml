        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />
    " />

    <Button

        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
